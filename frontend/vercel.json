{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "regions": ["sin1"],
  "env": {
    "NEXT_PUBLIC_API_URL": "@backend_url",
    "NEXT_PUBLIC_WS_URL": "@websocket_url"
  },
  "rewrites": [
    {
      "source": "/api/v1/:path*",
      "destination": "https://your-backend-url.com/api/v1/:path*"
    }
  ]
}
